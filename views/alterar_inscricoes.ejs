<!-- include header -->
<%- include('include/_header') %>
<!-- /include header -->


    <!-- Main Site -->
    <main id="site-main">
        <div class="container">

            <!-- form handling -->
            <form action="/" method="POST">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th>Nº Dupla</th>
                            <th>Nome</th>
                            <th>Email</th>
                            <th>Telemóvel</th>
                            <th>NIF</th>
                            <th>Ações</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%- include('include/_show') %>
                        <%for(var i= 0; i< User.length; i++) {%> 
                        <tr>
                            <td><%= i %></td>           
                            <td><%= User[i].name%></td>
                            <td><%= User[i].email%></td>
                            <td><%= User[i].phone%></td>
                            <td><%= User[i].nif%></td>
                            <td>
                                <a href="/" class="btn border-shadow update">
                                    <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
                                </a>
                                <a href="/apagar_inscricao" class="btn border-shadow delete">
                                    <span class="text-gradient"><i class="fas fa-times"></i></span>
                                </a>
                            </td>
                        </tr>
                        <%} %>
                    </tbody>
                </table>
            </form>
        </div>
    </main>
<!-- /Main Site -->


<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->